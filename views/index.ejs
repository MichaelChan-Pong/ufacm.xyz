<script>
 
 // ID of the Google Spreadsheet
 var spreadsheetID = "1p1S8Zak1kk9MpV_QEHlMfpbSGWBWjgMB4E8Po_bo7Sw/ozbi5jp";
 // Make sure it is public or set to Anyone with link can view 
 var url = "https://spreadsheets.google.com/feeds/list/" + spreadsheetID + "/public/values?alt=json";
 $.getJSON(url, function(data) {
  var entry = data.feed.entry;
  var i = 0;//incrementer, dont edit this
  var total = 0;//dont edit these, 
  var num = 1; //edit this for number of events to display
  $(entry).each(function(){
    total++;
    });
  $(entry).each(function(){
    if(total-(i+1) < num)
    {
      $('.results').prepend('<div class="row"><div class="col s12 m3"><p>'+this.gsx$date.$t+'</p><p> at&nbsp'+this.gsx$time.$t+'</p></div><div class="col s12 m3"></div><div class="col s12 m6"><h3>'+this.gsx$nameofevent.$t+'</h3><p></p>'+'<p>'+this.gsx$description.$t+'</p>'+'<div class="row"><div class="col s10"><p><i class="material-icons">location_on</i>'+this.gsx$location.$t+'</p></div><div class="col s2"><a href="'+this.gsx$facebookevent.$t+'"><img style="width:30px;"src="font/material-design-icons/social-media.svg"</a></div></div></div></div>');
    }
    i++;
  });
 
 });
 
</script>

